{
    "name": "yomikiru",
    "productName": "Yomikiru",
    "version": "2.12.0",
    "description": "App to read manga / comic / epub offline on desktop",
    "main": ".webpack/main",
    "author": {
        "name": "mienaiyami",
        "url": "https://github.com/mienaiyami/yomikiru"
    },
    "scripts": {
        "start": "electron-forge start",
        "eslint": "eslint",
        "tslint": "tsc --noemit",
        "lint": "eslint && tsc --noemit",
        "package": "electron-forge package",
        "make": "electron-forge make --arch=ia32",
        "make:exe": "electron-forge make --targets=@electron-forge/maker-squirrel --arch=ia32",
        "make:zip": "electron-forge make --targets=@electron-forge/maker-zip --arch=ia32",
        "make:deb": "electron-forge make --targets=@electron-forge/maker-deb",
        "release": "node makeAll.js"
    },
    "license": "MIT",
    "dependencies": {
        "@electron/remote": "^2.0.1",
        "@fortawesome/fontawesome-svg-core": "^6.2.1",
        "@fortawesome/free-brands-svg-icons": "^6.2.1",
        "@fortawesome/free-regular-svg-icons": "^6.2.1",
        "@fortawesome/free-solid-svg-icons": "^6.2.1",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@reduxjs/toolkit": "^1.9.3",
        "@vscode/sudo-prompt": "^9.3.1",
        "chokidar": "^3.5.3",
        "cross-zip": "^4.0.0",
        "css": "^3.0.0",
        "electron-dl": "^3.5.0",
        "electron-fetch": "^1.9.1",
        "electron-log": "^4.4.8",
        "electron-squirrel-startup": "^1.0.0",
        "font-list": "^1.4.5",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hot-loader": "^4.13.0",
        "react-redux": "^8.0.5"
    },
    "devDependencies": {
        "@babel/core": "7.14.6",
        "@babel/plugin-transform-runtime": "7.14.5",
        "@babel/preset-env": "7.14.5",
        "@babel/preset-react": "7.14.5",
        "@babel/preset-typescript": "7.18.6",
        "@electron-forge/cli": "6.0.4",
        "@electron-forge/maker-deb": "6.0.4",
        "@electron-forge/maker-rpm": "6.0.4",
        "@electron-forge/maker-squirrel": "6.0.4",
        "@electron-forge/maker-zip": "6.0.4",
        "@electron-forge/plugin-webpack": "6.0.4",
        "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
        "@types/cross-zip": "^4.0.0",
        "@types/css": "^0.0.33",
        "@types/electron-devtools-installer": "2.2.2",
        "@types/node": "^18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "babel-loader": "8.2.2",
        "cross-env": "7.0.3",
        "css-loader": "^6.4.0",
        "electron": "25.1.0",
        "eslint": "7.29.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-react": "7.24.0",
        "file-loader": "^6.2.0",
        "npm-run-all": "4.1.5",
        "sass": "^1.43.2",
        "sass-loader": "^12.2.0",
